<Page x:Class="newOnlineQA.pages.QA"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:newOnlineQA.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="QA">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid Background="#f1a0a3">
            <Button Style="{StaticResource RimlessButtonStyle}" Click="QuestionListClick" Width="50" Height="50" Margin="20 0 0 0" FontSize="36" Content="&#xe850;" FontFamily="../Fonts/#iconfont" Foreground="White" HorizontalAlignment="Left" BorderThickness="0" Background="Transparent" />
            <Button Style="{StaticResource RimlessButtonStyle}" Click="QuestionDetailsClick" Width="50" Height="50" Margin="0 0 20 0" FontSize="36" Content="&#xe60c;" FontFamily="../Fonts/#iconfont" Foreground="White" HorizontalAlignment="Right" BorderThickness="0" Background="Transparent" />
        </Grid>
        <Grid Grid.Row="1">
            <ContentControl x:Name="contentControl" />
        </Grid>
    </Grid>
</Page>
